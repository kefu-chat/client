{
  "name": "chat.phrassed",
  "version": "0.9.0",
  "license": "MIT",
  "scripts": {
    "build": "npm run widget && rollup -c && workbox injectManifest workbox-config.js",
    "dev": "npm run widget_dev && rollup -c -w",
    "bundle": "cross-env APP_ENV=production webpack --config webpack.extract.config.js --optimize-minimize --mode production",
    "widget": "cross-env APP_ENV=production webpack --config webpack.widget.config.js --optimize-minimize --mode production && cp src/*.css public/build/ && cp public/global.css public/build/ && cp public/*.svg public/build/ && cp src/*.svg public/build/ && cp src/*.svg public/ && cp public/demo.html public/build/ && cp src/*.css public/",
    "widget_dev": "webpack --config webpack.widget.config.js && cp src/*.css public/build/ && cp public/global.css public/build/ && cp public/*.svg public/build/ && cp src/*.svg public/build/ && cp src/*.svg public/ && cp public/demo.html public/build/ && cp src/*.css public/",
    "start": "sirv public"
  },
  "devDependencies": {
    "@hapi/hapi": "^19.1.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@rollup/plugin-replace": "^2.3.2",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "mini-css-extract-plugin": "^0.6.0",
    "prettier": "^2.0.5",
    "prettier-plugin-svelte": "^1.1.0",
    "rollup": "^2.7.2",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.1.2",
    "serve": "^11.0.0",
    "style-loader": "^0.23.1",
    "svelte": "^3.49.0",
    "svelte-loader": "2.13.3",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1",
    "workbox-cli": "^5.1.3"
  },
  "dependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "assets-webpack-plugin": "^5.1.2",
    "axios": "^0.21.2",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^3.0.0",
    "fs": "0.0.1-security",
    "laravel-echo": "^1.8.1",
    "sirv-cli": "^0.4.4",
    "socket.io-client": "^2.2.0",
    "uglify-js": "^3.11.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  }
}
